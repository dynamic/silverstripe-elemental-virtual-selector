{
    "name": "dynamic/silverstripe-elemental-virtual-selector",
    "description": "An enhancement to the virtual block selector. Filter by block type.",
    "license": "BSD-3-Clause",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "require": {
        "dnadesign/silverstripe-elemental-virtual": "^2",
        "sheadawson/silverstripe-dependentdropdownfield": "^3",
        "silverstripe/framework": "^5"
    },
    "require-dev": {
        "silverstripe/recipe-testing": "^3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dynamic\\VirtualSelector\\": "src/",
            "Dynamic\\VirtualSelector\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        }
    }
